{
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "eject": "expo eject",
    "test": "npm run tslint && npm run tsc-test && node node_modules\\jest\\bin\\jest.js",
    "tslint": "tslint --project .",
    "tsc-test": "tsc --project . --noEmit",
    "jest": "jest",
    "start:web": "webpack-dev-server"
  },
  "dependencies": {
    "@sensenet/client-core": "^1.3.2",
    "@sensenet/client-utils": "^1.4.1",
    "@sensenet/query": "^1.1.1",
    "expo": "^32.0.0",
    "html-webpack-plugin": "^3.2.0",
    "react": "^16.5.0",
    "react-art": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-native": "https://github.com/expo/react-native/archive/sdk-32.0.0.tar.gz",
    "react-native-web": "^0.10.0-alpha.1",
    "redux-di-middleware": "^2.0.2",
    "webpack-bundle-analyzer": "^3.0.3"
  },
  "devDependencies": {
    "@types/jest": "^23.3.12",
    "@types/react-dom": "^16.0.11",
    "@types/react-native": "^0.57.27",
    "@types/react-test-renderer": "^16.0.3",
    "awesome-typescript-loader": "^5.2.1",
    "copy-webpack-plugin": "^4.5.2",
    "jest": "^23.6.0",
    "jest-expo": "^32.0.0",
    "react-test-renderer": "^16.7.0",
    "source-map-loader": "^0.2.4",
    "ts-jest": "^23.10.5",
    "tslint": "^5.12.1",
    "typescript": "^3.2.2",
    "webpack": "^4.19.1",
    "webpack-cli": "^3.1.1",
    "webpack-dev-server": "^3.1.8"
  },
  "private": true,
  "jest": {
    "roots": [
      "<rootDir>/src",
      "<rootDir>/test"
    ],
    "preset": "jest-expo",
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/react-native/jest/preprocessor.js",
      "^.+\\.tsx?$": "ts-jest"
    },
    "testMatch": [
      "<rootDir>/test/*.+(ts|tsx|js)"
    ],
    "moduleFileExtensions": [
      "js",
      "ts",
      "tsx"
    ],
    "globals": {
      "ts-jest": {
        "tsConfig": {
          "jsx": "react",
          "module": "commonjs"
        }
      }
    },
    "collectCoverage": true,
    "collectCoverageFrom": [
      "<rootDir>/src/**/*.{ts,tsx}",
      "!<rootDir>/test/**/*.{ts,tsx}",
      "!**/*.d.{ts,tsx}",
      "!**/node_modules/**",
      "!**/index.ts"
    ],
    "coverageReporters": [
      "json",
      "html"
    ]
  }
}
